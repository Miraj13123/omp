{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "background": "#3A3A3A",
                    "foreground": "#3A86FF",
                    "style": "diamond",
                    "leading_diamond": "<#f1184c,transparent>\u256d\u2500</>\ue0b2",
                    "trailing_diamond": "",
                    "properties": {
                        "alpine": "\uf300",
                        "arch": "\uf303",
                        "centos": "\uf304",
                        "debian": "\uf306",
                        "elementary": "\uf309",
                        "fedora": "\uf30a",
                        "gentoo": "\uf30d",
                        "linux": "\ue712",
                        "macos": "\ue711",
                        "manjaro": "\uf312",
                        "mint": "\uf30f",
                        "opensuse": "\uf314",
                        "raspbian": "\uf315",
                        "ubuntu": "\uf31c",
                        "windows": "\ue70f"
                    },
                    "template": " {{ if .WSL }}WSL at {{ end }}{{ if eq .OS \"debian\" }}<#FF0000>{{.Icon}}</>{{ else }}{{.Icon}}{{ end }} ",
                    "type": "os"
                },
                {
                    "background": "#242424",
                    "foreground": "#3A86FF",
                    "powerline_symbol": "\ue0b0",
                    "style": "diamond",
                    "leading_diamond": "\ue0b2",
                    "trailing_diamond": "",
                    "template": " {{ .UserName }} ",
                    "type": "session"
                },
                {
                    "background": "#242424",
                    "foreground": "#f1184c",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": "- root \uf0e7 ",
                    "type": "root"
                },
                {
                    "background": "#3A3A3A",
                    "foreground": "#FFFFFF",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "folder_separator_icon": "/",
                        "style": "full"
                    },
                    "style": "powerline",
                    "template": " \ue5ff {{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "#242424",
                    "foreground": "#FF4040",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "powerline",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "type": "git"
                },
                {
                    "background": "#66B3FF",
                    "background_templates": [
                        "{{ if gt .Code 0 }}#FFFF00{{ end }}"
                    ],
                    "foreground": "#242424",
                    "style": "diamond",
                    "leading_diamond": "<transparent,background>\ue0b0</>",
                    "trailing_diamond": "\ue0b0",
                    "properties": {
                        "always_enabled": true
                    },
                    "template": " \uea6c ",
                    "type": "status"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "overflow": "hide",
            "segments": [
                {
                    "background": "#e4e4e4",
                    "foreground": "#585858",
                    "properties": {
                        "style": "austin",
                        "always_enabled": true
                    },
                    "invert_powerline": true,
                    "style": "powerline",
                    "powerline_symbol": "\ue0b2",
                    "template": " \ueba2 {{ .FormattedMs }} ",
                    "type": "executiontime"
                },
                {
                    "background": "#0087af",
                    "foreground": "#f2f3f8",
                    "invert_powerline": true,
                    "style": "powerline",
                    "powerline_symbol": "\ue0b2",
                    "template": " \ue795 {{ .Name }} ",
                    "type": "shell"
                },
                {
                    "background": "#378504",
                    "foreground": "#f2f3f8",
                    "invert_powerline": true,
                    "style": "powerline",
                    "powerline_symbol": "\ue0b2",
                    "properties": {
                        "display_mode": "compact"
                    },
                    "template": " \ue266 {{ printf \"%.0f\" .PhysicalPercentUsed }}% ",
                    "type": "sysinfo"
                },
                {
                    "background": "#d75f00",
                    "foreground": "#f2f3f8",
                    "properties": {
                        "time_format": "3:04 PM"
                    },
                    "invert_powerline": true,
                    "style": "diamond",
                    "template": " \uf073 {{ .CurrentDate | date .Format }} ",
                    "trailing_diamond": "\ue0b0",
                    "type": "time"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#f1184c",
                    "style": "plain",
                    "template": "\u2570\u2500\u279c ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 3
}